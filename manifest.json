{
  "manifest_version": 3,
  "name": "XReposter",
  "version": "1.0.1",
  "description": "Post any X / Twitter media without downloading",
  "author": "GopiWebDev",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "index.html",
    "default_title": "XReposter"
  }
}
